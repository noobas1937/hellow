.selectarea{
  width: 100%;
  display: flex;
  background-color: #fff;
  flex-direction: column;
  justify-content: center;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  height: 40vh;
  opacity: 1;
  z-index:100;
}

.picker-item {
  line-height: 70rpx;
  margin-left: 5rpx;
  margin-right: 5rpx;
  text-align: center;
}
.cancelBtn{
    width:100rpx;
    height:50rpx;
    position:absolute;
    left:0;
    top:0;
    z-index:200;
    font-size: 30rpx;
    color:#949090;
    text-align: center;
    line-height: 40rpx;
}
.msureBtn{
    width:100rpx;
    height:50rpx;
    position:absolute;
    right:0;
    top:0;
    z-index:200;
    font-size: 30rpx;
    color:#098833;
    text-align: center;
    line-height: 40rpx;
}
.a1{
  display: block;
}
.a2{
  display: none;
}