.box{
  
    width:48rpx;
    height:48rpx;
    border-radius: 50%;
    border: 1rpx solid #cccccc;
    float:right;
    margin-top:-10rpx;
    
}
.box1{
    width:48rpx;
    height:48rpx;
    border-radius: 50%;
    border: 1rpx solid #cccccc;
    float:right;
    margin-top:-10rpx;
     background: url('../../../img/msure.png')no-repeat center;
    background-size: 48rpx;
}
.nwa-1c2{
    display: block;
